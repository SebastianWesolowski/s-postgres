NGROK_AUTH_TOKEN=your_auth_token_here
FORCE_COLOR=1
STRICT_RULES=false
SDEBUG="false"
TEST_WITH_SNAPSHOTS="false"

ANALYZE=false
LOCAL_ENV_VARIABLE="server_only_development_variable - LOCAL"

# Option 1: Full database URL
DATABASE_URL=postgresql://test:test@localhost:5432/mydb

# Option 2: Individual variables
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=test
DATABASE_PASSWORD=test
DATABASE_NAME=mydb

# Docker container name
CONTAINER_NAME="s-postgres-mydb"
